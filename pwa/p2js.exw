function tokenize(s, esc, sep) {
    let a = [],
        t = '',
        e = s.length;
    for (let i=0; i<e; i+=1) {
        let c = s.charAt(i)
        if (c == esc) t+=s.charAt(++i)
        else if (c != sep) t+=c
        else a.push(t), t=''            
