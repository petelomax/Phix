function mls(sequence s)
--  s = {{91'[',{{91'[',{{91'[',{{4,51'3',51'3',3,12,25936},
--                               {3,53'5',53'5',3,7}}},
--                       {3,56'8',56'8',3,10}}},
--               {3,59';',59';',3,13}}}}
    integer toktype = s[TOKTYPE]
    if toktype=LETTER then
        return get_name(s)
    elsif toktype='[' then
        string v = mls(s[2][1]),
               dx = emit_expr(s[2][2],0)
        return v & "[" & dx & "]"
    else
        ?9/0
    end if
end function

