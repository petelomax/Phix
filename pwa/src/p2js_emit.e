        integer sdx = find(tok[TOKTTIDX],static_tids)
        if sdx then
            if oneline = `    const `
            or oneline = `        const ` then
--              oneline = `    /*constant*/ `
                ?9/0 -- (caller must do "if (!x) { x = expr; }", via get_static(), instead)
            end if
            res = static_names[sdx]
        end if
    end if
--  if res="$" then res = "-1" end if
    return res
end function

