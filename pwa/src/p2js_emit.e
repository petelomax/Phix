    args = args[2]
--DEV or, append comment & flush oneline as we find them...
--  args = {{4,1476,1481,36'$',15,544},{4,1483,1487,36'$',15,16936},{},{5,1492,1512,36'$',28}}
    for i=length(args) to 1 by -1 do
