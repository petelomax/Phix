function add_includes(sequence ai, axtra)
    for i=1 to length(axtra) do
        string axi = axtra[i]
        if not find(axi,ai) then
            ai = append(ai,axi)
        end if
    end for
    return ai
end function

global function generate_source(sequence ast, integer oxt, src_offset, bool pGUI, xpGUI, pMPFR, pSHA256, pTIMEDATE)
--global function generate_source(sequence ast, integer oxt, src_offset, bool pGUI, pOpenGL?, pMPFR, pSHA256, pTIMEDATE)
    wasext = ext    -- save input language
    ext = oxt       -- set output language, so we can use is_phix() etc.
    initp = iff(is_phix()?0:12)
--  massN = 0
--  massn = {}
--?initp
    output = ""
    tokline = src_offset+1
    bGUI = false
    bxpGUI = false
    bOpenGL = false
    bMPFR = false
    bSHA256 = false
    bTIMEDATE = false
    next_static_id = 1
    static_tids = {}
--trace(1)
    emit(ast[2])
    flush_oneline()
--erm, we may well need to stitch several sources together...
    if is_html() then
        sequence hdr_args = {"",""}
        if bGUI or pGUI then
            if bOpenGL then
                hdr_args = {pGUIcs,pGUIjs&OpenGL}
            else
                hdr_args = {pGUIcs,pGUIjs}
            end if
        end if
        if bxpGUI or xpGUI then
            if bOpenGL then
                hdr_args = {xpGUIcs,xpGUIjs&OpenGL}
            else
                hdr_args = {xpGUIcs,xpGUIjs}
            end if
        end if
        string header = sprintf(html_headerf,hdr_args),
               aincs = ""
        sequence ai = get_autoincludes()[1]
        if bGUI or pGUI
        or bxpGUI or xpGUI then
            ai = add_includes(ai,{"find.e","pmaths.e","ptagset.e"})
        elsif bOpenGL then
            ?9/0
        end if
        if bMPFR or pMPFR then
            ai = add_includes(ai,{`find.e`,`pmaths.e`,`primes.e`,`ptagset.e`,`match.e`,`substitute.e`,`mpfr.e`})
        end if
        if bSHA256 or pSHA256 then
            ai = add_includes(ai,{"sha256.e"})
        end if
--DEV erm, why??
        if bTIMEDATE or pTIMEDATE then
            ai = add_includes(ai,{"pcase.e","pdates.e","pmaths.e","ptagset.e","pcolumn.e","timedate.e"})
        end if
        for i=1 to length(ai) do
            if ai[i]!="==" then
                string js = "builtins/"&substitute(ai[i],".e",".js")
                aincs &= sprintf("\n  <scr!ipt src=\"%s\"></scr!ipt>",{js})
            end if
        end for
        header = sprintf(header,{get_file_name(current_file),aincs})
        header = substitute(header,"scr!ipt","script")
        header &= use_strict
        header &= output
        header &= substitute(html_footer,"scr!ipt","script")
        output = header
    elsif is_js() then
        output = use_strict & output
    elsif is_phix() then
        if pGUI and not bGUI then
            output = "include pGUI.e\n" & output
        end if
        if xpGUI and not bxpGUI then
            output = "include xpGUI.e\n" & output
        end if
--      if pOpenGL and not bOpenGL then
--          output = `include ..\pGUI\opengl.e` & "\n" & output
--      end if
        if pMPFR and not bMPFR then
            output = "include mpfr.e\n" & output
        end if
        if pSHA256 and not bSHA256 then
            output = "include sha256.e\n" & output
        end if
        if pTIMEDATE and not bTIMEDATE then
            output = "include timedate.e\n" & output
        end if
    end if
    ext = wasext    -- restore input language
    return output
end function

