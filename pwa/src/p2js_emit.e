procedure flush_oneline()
    if length(oneline) then
        if oneline[$]!=nl then
            oneline &= nl
        end if
--NO:
--      if tokline>prevline+1 then
--          oneline &= nl
--          prevline = tokline
--      end if
        output &= oneline
        oneline = ""
    end if
end procedure

