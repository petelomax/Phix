function ident(sequence tok)
    if find(tok[TOKTYPE],{LETTER,DIGIT,HEXSTR}) then
        integer {?,tokstart,tokfinish} = tok
        return src[tokstart..tokfinish]
    end if
    return ""
end function

function get_name(sequence tok)
