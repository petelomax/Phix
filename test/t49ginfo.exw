-- remainder(-6..-5,unk) is -6..0   (km6a)
-- remainder(-6..5,unk) is -6..5    (km65)
-- remainder(-5..5,unk) is -5..5    (km5a)
-- remainder(-4..3,unk) is -4..3    (km4a)
