--
-- t15inc9.exw
--
-- Phix include file testing
--
-- Specifically aimed at the global symbol table resequencing 
-- which occurs at the end of each include file.
-- This program should work equally on Phix and RDS.
--
--  t15inc9: a=1, t15inc9b.e, e=5, t15inc9f.e, g=7
--  t15inc9b.e: b=2, t15inc9c.e, d=4
--  t15inc9c.e: c=3
--  t15inc9f.e: f=6
--
without warning -- lots of unused stuff in here

global constant a=1
include t15inc9b.e
global constant e=5
include t15inc9f.e
global constant g=7
--if {a,b,c,d,e,f,g}!={1,2,3,4,5,6,7} then crash("{a,b,c,d,e,f,g}!={1,2,3,4,5,6,7}\n") end if
if a!=1 then crash("a!=1\n") end if
if b!=2 then crash("b!=2\n") end if
if c!=3 then crash("c!=3\n") end if
if d!=4 then crash("d!=4\n") end if
if e!=5 then crash("e!=5\n") end if
if f!=6 then crash("f!=6\n") end if
if g!=7 then crash("g!=7\n") end if
include t15inc9.exw as M
include t15inc9b.e as B
include t15inc9c.e as C
include t15inc9f.e as F
--if {M:a,B:b,C:c,B:d,M:e,F:f,M:g}!={1,2,3,4,5,6,7} then
--  crash("{M:a,B:b,C:c,B:d,M:e,F:f,M:g}!={1,2,3,4,5,6,7}\n")
--end if
if M:a!=1 then crash("M:a!=1\n") end if
if B:b!=2 then crash("B:b!=2\n") end if
if C:c!=3 then crash("C:c!=3\n") end if
if B:d!=4 then crash("B:d!=4\n") end if
if M:e!=5 then crash("M:e!=5\n") end if
if F:f!=6 then crash("F:f!=6\n") end if
if M:g!=7 then crash("M:g!=7\n") end if

--if getc(0) then end if
