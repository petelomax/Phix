procedure SetTabIdx(string name)
--integer id
Ihandle tab
    ?{"SetTabIdx",name}
    tabidx = find(name,tabnames)
    if tabidx=0 then
        tabnames = append(tabnames,name)
        tabidx = length(tabnames)
-->     id = create(TabItem,name,0,TabCtrl,0,0,0,0,0)
        tab = IupHbox({IupFill()})
--      tabids = append(tabids,id)
