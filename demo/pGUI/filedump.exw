    end if
    file_size = where(fn)
--printf(1,"filesize:#%08x\n",file_size)
    if seek(fn,0)!=0 then
        close(fn)
        loadError("error seeking to start of file")
        return
    end if
    binary = repeat(' ',file_size)
    for i=1 to file_size do
        binary[i] = getc(fn)
    end for
    close(fn)

