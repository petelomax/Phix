function ds_normalise(atom p_addr)
atom vaddr
    for i=1 to length(p_vaddrs) do
        vaddr = p_vaddrs[i]
        if p_addr>=vaddr and p_addr<vaddr+p_fileszs[i] then
            p_addr -= (vaddr-p_offsets[i])
            return p_addr
        end if
    end for
--  return 9/0
    return 0
end function

