--      for a=address to addrMax do
--          byte = binary[a+1]
--          hex &= sprintf("%02x",byte)
--          if remainder(a+5,4)=0 then
--              hex &= ' '
--          end if
--          if byte<' ' or byte>#7E then byte = '.' end if
--          text &= byte
--      end for
--      line = {address,"-","-",hex,text}
--      res[tabidx] = append(res[tabidx],line)
--      address = addrMax+1
--  end while
--end if
    return 1
end function


