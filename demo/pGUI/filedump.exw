--      // char *shift[] = { "LSL", "LSR", "ASR",  "RR" };
--      char *shift[] = { "<<", ">>", "->>", ">>>" };
--      unsigned int Rn, Rd, Rs, Rm;
--      unsigned int adr1;
--      int adrinstr;
--      int trouve;
--
--      adrinstr = adr;
--
--      Rn = field (x, 0xF0000);
--      Rd = field (x, 0xF000);
