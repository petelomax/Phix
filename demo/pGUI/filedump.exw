                    if equal(si[S_Name],"-1") then          -- S_Efct is meaningless on tls
                        sState = "0"
                    else
                        siState = si[S_Efct]
                        if siState=E_none then
                            sState = "E_none"
                        elsif siState=E_other then
                            sState = "E_other"
                        elsif siState=E_all then
                            sState = "E_all"
                        else
                            sState = sprintf("#%08x",siState)
                        end if
                    end if
