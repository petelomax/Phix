function reid_to_cube(string reid)
string cube = init
--  cube = substitute_all(cube,"YBRGOW","ybrgow")   -- (test)
    reid = substitute(reid," ","")
    for i=1 to length(reid) do
        cube[reidmap[i]] = "YBRGOW"[find(reid[i],"ULFRBD")]
    end for
    return cube
end function

