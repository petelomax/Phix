function cube_to_reid(string cube)
string res = ""
    for i=1 to length(reidmap) do
        if i<=25 then
            if i>1 and mod(i,2)=1 then res &= ' ' end if
        elsif mod(i,3)=1 then
            res &= ' '
        end if
        res &= "ULFRBD"[find(cube[reidmap[i]],"YBRGOW")]
    end for
    return res
end function

